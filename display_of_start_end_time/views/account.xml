<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   <record id="inherit_view_timesheet" model="ir.ui.view">
        <field name="name">account.analytic.line.inherit</field>
        <field name="model">account.analytic.line</field>
        <field name="inherit_id" ref="timesheet_grid.timesheet_view_grid_by_project"/>
        <field name="arch" type="xml">
           <xpath expr="//field[(@name='task_id')]" position="after">
               <field name="_date_start"/>
               <field name="_date_end"/>
               
           </xpath>
        </field>
    </record>

</odoo>

              
       

