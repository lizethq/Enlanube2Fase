<?xml version="1.0" encoding="UTF-8"?>
<odoo>
   <record id="inherit_view_project_task" model="ir.ui.view">
        <field name="name">project.task.inherit</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_form2"/>
        <field name="arch" type="xml">
           <xpath expr="//field[(@name='tag_ids')]" position="after">
               <field name="email2"/>
               <field name="number"/>
           </xpath>
        </field>
    </record>

    <record id="sequence_project_task_lfpv_introdoo" model="ir.sequence">
           <field name="name">Secuencia tareas lfpv</field>
           <field name="code">task.lfpv</field>
           <field name="active">TRUE</field>
           <field name="prefix">TAREA</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
    </record> 

</odoo>

              
       

